[% META charts = 1 %]

<p>this is the datasets explore json template</p>
[% FOREACH item IN facts %]

  [% IF item.fact_type == 'population' %]
    [% INCLUDE 'datasets/json_population.tt2' %]
  [% END %]

  [% IF item.fact_type == 'sample_types' %]
    [% INCLUDE 'datasets/json_sample_types.tt2' %]
  [% END %]

[% END %]

