[% META title="Notes App" %]
<div ng-app="notesApp">
  <div ng-controller="MainCtrl as ctrl">
<input type="text" ng-model="ctrl.username">
<input type="password" ng-model="ctrl.password">
<button ng-click="ctrl.change()">Change Values</button>
<button ng-click="ctrl.submit()">Submit</button>


<script src="[% site.url.js %]/angular1.3.11/angular.js"></script>
<script type="text/javascript" src="[% site.url.js %]/ng_test.js">
</script>

  </div><!-- end ng-controller div -->
</div><!-- end ng-app div -->
