<div class="table-responsive">
  <table class="table table-striped text-left">
  <caption>Data Requested</caption>
    <thead>
      <tr>
        <th>Category</th>
        <th>Request</th>
      </tr>
    </thead>
    <tbody>
[% FOREACH item IN data_items %]
      <tr>
        <td>[% item.data_category.friendly_category %]</td>
        <td>[% item.detail %]</td>
      </tr>
[% END %]
    </tbody>
  </table>
</div>
