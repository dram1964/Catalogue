
	<div class="row">
          <div class="form-group">
            <label>Do you require data with patient identifiers?</label>
            <label class="radio-inline"><input type="radio" ng-model="ctrl.data.identifiable" name="identifiable" value="1">Yes</label>
            <label class="radio-inline"><input type="radio" ng-model="ctrl.data.identifiable" name="identifiable" value="0">No</label>
          </div><!-- end form-group -->
	</div><!-- end row -->

	<div class="row">
	  <div class="form-group" ng-show="ctrl.data.identifiable == 1">
	    <div class="col-md-3">
<input type="checkbox" name="identifiers" 
ng-checked="ctrl.data.identifiers.ptName"
value="ptName">Patient Name<br>
	    </div><!-- end columnn -->
	    <div class="col-md-3">
<input type="checkbox" name="identifiers" 
ng-checked="ctrl.data.identifiers.dob"
value="dob">Patient Date of Birth<br>
	    </div><!-- end columnn -->
	    <div class="col-md-3">
<input type="checkbox" name="identifiers" 
ng-checked="ctrl.data.identifiers.addr"
value="addr">Patient Address<br>
	    </div><!-- end columnn -->
	    <div class="col-md-3">
<input type="checkbox" name="identifiers" 
ng-checked="ctrl.data.identifiers.ptNumber"
value="ptNumber">Patient Hospital Number<br>
	    </div><!-- end columnn -->
	    <div class="col-md-3">
<input type="checkbox" name="identifiers" 
ng-checked="ctrl.data.identifiers.nhsNumber"
value="nhsNumber">Patient NHS Number<br>
	    </div><!-- end columnn -->
	    <div class="col-md-12">
<br><label>Additional Identifiers:</label>
	    </div><!-- end columnn -->
<textarea name="identifiableSpecification" ng-model="ctrl.data.identifiableSpecification" 
	class="form-control"></textarea>
	  </div><!-- end form-group -->
	</div><!-- end row -->

        <div class="form-group">
<label>Will you be publishing patient identifiers to any third parties?</label>
<label class="radio-inline"><input type="radio" ng-model="ctrl.data.publishId" name="publish" value="1">Yes</label>
<label class="radio-inline"><input type="radio" ng-model="ctrl.data.publishId" name="publish" value="0">No</label>
        </div><!-- end form-group -->

        <div class="form-group" ng-show="ctrl.data.publishId == 1">
	    <div class="col-md-12">
<br><label>Please specify all third-parties you intend to publish patient identifiers to:</label>
	    </div><!-- end columnn -->
<textarea name="publishIdSpecification" ng-model="ctrl.data.publishIdSpecification" 
	class="form-control"></textarea>
        </div><!-- end form-group -->

        <div class="form-group">
<label>Where will you be storing and processing this data</label>
<input type="textarea" name="storing" ng-model="ctrl.data.storing"
	class="form-control"></textarea>
        </div><!-- end form-group -->

        <div class="form-group">
<label>What will you do with the data once you have completed the project</label>
<input type="textarea" name="completion" ng-model="ctrl.data.completion"
	class="form-control"></textarea>
        </div><!-- end form-group -->

        <div class="form-group">
<label>Additional information:</label>
<input type="textarea" name="additional" ng-model="ctrl.data.additional"
	class="form-control"></textarea>
        </div><!-- end form-group -->
